{
  "manifest_version": 2,
  "name": "Inappropriate Language Blocker",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": {
      "16": "icon.png"
    }
  },
"permissions": ["activeTab"],
  "icons": {
        "16": "icon.png",
        "32": "icon2.png",
        "64":"icon3.png"
      },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}
